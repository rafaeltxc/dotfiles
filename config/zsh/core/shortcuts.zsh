# Vpn
alias vpn="sudo openvpn --config ~/Documents/Work/vpn.ovpn"

# Tomcat
alias cats="sudo /opt/tomcat/bin/startup-debug.sh"
alias catt="sudo /opt/tomcat/bin/shutdown.sh"

